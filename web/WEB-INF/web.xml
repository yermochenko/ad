<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
	<display-name>Документооборот кафедры</display-name>

	<servlet>
		<servlet-name>MainPage</servlet-name>
		<servlet-class>ad.servlets.MainPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MainPage</servlet-name>
		<url-pattern>/index.html</url-pattern>
		</servlet-mapping>
		
	<servlet>
		<servlet-name>CurriculumsServlet</servlet-name>
		<servlet-class>ad.servlets.CurriculumsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CurriculumsServlet</servlet-name>
		<url-pattern>/curriculums.html</url-pattern>
		</servlet-mapping>
		
	<servlet>
		<servlet-name>CurriculumDisciplinesServlet</servlet-name>
		<servlet-class>ad.servlets.CurriculumDisciplinesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CurriculumDisciplinesServlet</servlet-name>
		<url-pattern>/curriculumdisciplines.html</url-pattern>
		</servlet-mapping>
		
	<servlet>
		<servlet-name>DisciplinesCheckServlet</servlet-name>
		<servlet-class>ad.servlets.disciplinesservlets.DisciplinesCheckServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisciplinesCheckServlet</servlet-name>
		<url-pattern>/check.html</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DisciplinesDeleteServlet</servlet-name>
		<servlet-class>ad.servlets.disciplinesservlets.DisciplinesDeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisciplinesDeleteServlet</servlet-name>
		<url-pattern>/delete.html</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DisciplinesEditServlet</servlet-name>
		<servlet-class>ad.servlets.disciplinesservlets.DisciplinesEditServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisciplinesEditServlet</servlet-name>
		<url-pattern>/edit.html</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DisciplinesSaveservlet</servlet-name>
		<servlet-class>ad.servlets.disciplinesservlets.DisciplinesSaveServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisciplinesSaveservlet</servlet-name>
		<url-pattern>/save.html</url-pattern>
	</servlet-mapping>
		
	<servlet>
		<servlet-name>DisciplinesServlet</servlet-name>
		<servlet-class>ad.servlets.disciplinesservlets.DisciplinesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisciplinesServlet</servlet-name>
		<url-pattern>/disciplines.html</url-pattern>
		</servlet-mapping>
		
	<servlet>
		<servlet-name>ViewDisciplinesEditServlet</servlet-name>
		<servlet-class>ad.servlets.disciplinesservlets.ViewDisciplinesEditServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewDisciplinesEditServlet</servlet-name>
		<url-pattern>/WEB-INF/edit.html</url-pattern>
	</servlet-mapping>
		
	<servlet>
		<servlet-name>ViewDisciplinesServlet</servlet-name>
		<servlet-class>ad.servlets.disciplinesservlets.ViewDisciplinesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewDisciplinesServlet</servlet-name>
		<url-pattern>/WEB-INF/disciplines.html</url-pattern>
		</servlet-mapping>
		
	<servlet>
		<servlet-name>SpecialtiesServlet</servlet-name>
		<servlet-class>ad.servlets.SpecialtiesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SpecialtiesServlet</servlet-name>
		<url-pattern>/specialties.html</url-pattern>
		</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
